public class CodeWordChecker implements StringChecker{ 
  int minLen;
  int maxLen;
  String ill;

  public CodeWordChecker(int first, int second, String str){
    minLen = first;
    maxLen = second;
    ill = str;
  }
  public CodeWordChecker(String str){
    ill = str;
    min = 6;
    max = 20;
  }
  
  public boolean isValid(String str){
    if(str.length() > min || str.length() < 20){
      return false;
    }
    else{
      if(str.indexOf(ill) != -1){
        return false;
      }
    }
    return true;
  }
}
